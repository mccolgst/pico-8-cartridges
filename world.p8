pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
t=0
fires={}
buildings={}
ppl={}
missiles={}
missile_speed=1.5
fire_speed=2
p = {}
p.x=50
p.y=50
p.speed=1
p.dy=0
p.dx=0
p.s=1
p.frames=4
p.t=0

function _init()
  for i=0,10 do
    create_buildings()
  end
  for i=0,5 do
    create_missile()
  end
end

function _update()
  t+=1
  p.dx=0
  p.dy=0
  if btnp(4) then
    printh("shoot!")
    shoot()
  end
  if btn(0) then
    p.dx=-p.speed
  elseif btn(1) then
    p.dx=p.speed
  end
  
  
  if btn(2) then
    // move player up
    p.dy=-p.speed
  elseif btn(3) then
    // move player down
    p.dy=p.speed
  else
    p.dy=0
  end
  


  if t%5==0 then
    p.s=1+p.t%p.frames
  end

  if p.y<8 then
    p.dy=0
  end
  if p.y>128 then
    p.dy=0
  end
  if p.x<8 then
    p.dx=0
  end
  p.y+=p.dy
  p.x+=p.dx
  p.t+=1


  for fire in all(fires) do
    fire.t+=1
    fire.s=33+fire.t%fire.frames
    fire.x+=fire.dx
    if fire.x>128 then
      fire.done=true
    end
    for b in all(buildings) do
      if b.destroyed == false then
        if check_collision(fire, b) then
          fire.done=true
          //play noise
          //destroy building
          b.destroyed=true
          b.s=flr(rnd(4))+16
          //create running ppl
          add({x},ppl)
        end
      end
    end
  end
  for m in all(missiles) do
    m.t+=1
    m.x-=m.dx
    m.s=38+m.t%m.frames
    if m.x < 0 then
      m.x+=128+rnd(128)
      m.y=rnd(128)
    end
  end
  for fire in all(fires) do
    if fire.done then
      del(fires,fire)
    end
  end
  for b in all(buildings) do
    b.x-=1
    if b.x<0 then
      del(buildings,b)
    end
  end
  if t%100==0 then
    for i=0,10 do
      create_buildings()
    end
  end
end

function _draw()
  cls()

  for b in all(buildings) do
    spr(b.s,b.x,b.y)
  end
  for fire in all(fires) do
    spr(fire.s,fire.x,fire.y)
  end
  for m in all(missiles) do
    printh("ms "..m.s.." mx "..m.x.." my "..m.y)
    spr(m.s,m.x,m.y)
  end
  spr(p.s,p.x,p.y)
  print("x:"..p.x,20,0)
  print("y:"..p.y,20,10)
end

function shoot()
  b = {}
  b.frame=0
  b.spr_l=33
  b.frames=4
  b.x=p.x+7
  b.y=p.y
  b.dx=fire_speed
  b.done=false
  b.t=0
  b.s=33
  add(fires,b)
end

function create_buildings()
  b = {}
  b.x=p.x+128+rnd(128)
  b.y=rnd(128)
  b.w=8
  b.h=8
  b.destroyed=false
  sp=flr(rnd(3))
  b.s=sp+49
  add(buildings,b)
end

function create_missile()
  m = {}
  m.x=rnd(128)+128
  m.dx=missile_speed
  m.y=rnd(128)
  m.frames=4
  m.s=38
  m.t=0
  add(missiles,m)
end

function check_collision(fire, b)
  if fire.x < b.x+b.w and
     fire.x+8 > b.x and
     fire.y+8 > b.y and
     fire.y < b.y+b.h and
     fire.y+8 > b.y then
     
    return true
  end
  return false
end
