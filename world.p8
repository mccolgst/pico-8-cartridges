pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
t=0
score=0
fires={}
buildings={}
ppl={}
missiles={}
missile_speed=1.5
fire_speed=2
p = {}
p.x=50
p.y=50
p.speed=.75
p.dy=0
p.dx=0
p.s=1
p.frames=4
p.t=0
p.health=4
p.invuln=false
p.invuln_cd=0
p.fire_cd=0


function _init()
music(0)
  for i=0,10 do
    create_buildings()
  end
  for i=0,5 do
    create_missile()
  end
end

function _update()
  t+=1
  score+=flr(rnd(2))
  p.dx=0
  p.dy=0
  if btnp(4) and p.t > p.fire_cd then
    shoot()
  end
  if btn(0) then
    p.dx=-p.speed
  elseif btn(1) then
    p.dx=p.speed
  end
  
  
  if btn(2) then
    // move player up
    p.dy=-p.speed
  elseif btn(3) then
    // move player down
    p.dy=p.speed
  else
    p.dy=0
  end
  
  if t%5==0 then
    p.s=1+p.t%p.frames
  end

  if p.x < 8 and not p.invuln then
    hurt_player()
  end
  if p.invuln_cd<p.t then
    p.invuln=false
  end
  for fire in all(fires) do
    fire.t+=1
    fire.s=33+fire.t%fire.frames
    fire.x+=fire.dx
    if fire.x>128 then
      fire.done=true
    end
    for b in all(buildings) do
      if b.destroyed == false then
        if check_collision(fire, b) then
          fire.done=true
          //play noise
          //destroy building
          b.destroyed=true
          score+=flr(rnd(20))+50
          sfx(0)
          b.s=flr(rnd(4))+16
          //create running ppl
          add({x},ppl)
        end
      end
    end
  end
  for m in all(missiles) do
    if check_collision(p,m) then
      hurt_player()
      m.x+=128+rnd(128)
      m.y=rnd(128)
      m.dx=1+rnd(missile_speed)
    end
    m.t+=1
    m.x-=m.dx
    m.s=38+m.t%m.frames
    if m.x < 0 then
      m.x+=128+rnd(128)
      m.y=rnd(128)
      m.dx=1+rnd(missile_speed)
    end
  end
  for fire in all(fires) do
    if fire.done then
      del(fires,fire)
    end
  end
  for b in all(buildings) do
    if b.destroyed==false and check_collision(p,b) then
      p.dx=-1
    end
    b.x-=1
    if b.x<0 then
      del(buildings,b)
    end
  end
  if t%100==0 then
    for i=0,10 do
      create_buildings()
    end
  end

  p.y+=p.dy
  p.x+=p.dx
  p.t+=1

end

function _draw()
  cls()

  for b in all(buildings) do
    spr(b.s,b.x,b.y)
  end
  for fire in all(fires) do
    spr(fire.s,fire.x,fire.y)
  end
  for m in all(missiles) do
       spr(m.s,m.x,m.y)
  end
  if p.invuln and t%5==0 then
    p.s=3+p.t%p.frames
  end
  if p.t<p.fire_cd then
    draw_fire_cd()
  end
  spr(p.s,p.x,p.y)
  
  // draw fire chasing
  for i=0,16 do
    spr(flr(rnd(4))+22,0,i*8)
  end
  
  draw_hud()
  if p.health<=0 then
    game_over()
  end
end

function shoot()
  b = {}
  b.frame=0
  b.spr_l=33
  b.frames=4
  b.x=p.x+7
  b.y=p.y
  b.dx=fire_speed
  b.done=false
  b.t=0
  b.s=33
  p.fire_cd=(1*30)+p.t
  add(fires,b)
  sfx(1)
end

function create_buildings()
  b = {}
  b.x=p.x+128+rnd(128)
  b.y=rnd(128)
  b.w=8
  b.h=8
  b.destroyed=false
  sp=flr(rnd(3))
  b.s=sp+49
  add(buildings,b)
end

function create_missile()
  m = {}
  m.x=rnd(128)+128
  m.dx=1+rnd(missile_speed)
  m.y=rnd(128)
  m.frames=4
  m.s=38
  m.t=0
  m.w=8
  m.h=4
  add(missiles,m)
end

function check_collision(a, b)
  if a.x < b.x+b.w and
     a.x+8 > b.x and
     a.y+8 > b.y and
     a.y < b.y+b.h and
     a.y+8 > b.y then
     
    return true
  end
  return false
end

function draw_hud()

  //health
  rect(10,2,35,6,6)
  rectfill(11,3,p.health*8.75-1,5,3)

  //score
  print(score,90,3,6)
end

function draw_fire_cd()
  rectfill(p.x-5,p.y+13, (p.x-5+(p.fire_cd-p.t)/2), p.y+12,3)
end

function hurt_player()
  if p.invuln==false then
    p.health-=1
    p.invuln=true
    p.invuln_cd=(3*30)+p.t
    sfx(2)
  end
end

function game_over()
  cls()
  print("you have been vanquished!",128/2-30,128/2,6)
end
