pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
player = {}
player.x = 2
player.y = 2
player.dx = 0
player.dy = 0
player.sprite = 1
player.speed = 1

level = 0
level_startx = (level * 16)

bldr_spr = 6
hole_spr = 9
filled_hole_spr = 8
ground_spr = 4
ladder_spr = 23

t = 0

function player_init_pos()
  for y=0,16 do
    for x=level_startx,level_startx+16 do
      val = mget(x, y)
      printh("x "..x.." y "..y.." val "..val)
      if val==ground_spr then
        return x,y
      end
    end
  end
  return 0,0
end

function _init()
  --find adequate pos for player
  player.x, player.y = player_init_pos()  
end

function _update()
  if btnp(0) then
    player.dx = -player.speed
    player.sprite = 1  
  end
  if btnp(1) then 
    player.dx = player.speed
    player.sprite = 17
  end
  if btnp(2) then 
    player.dy = -player.speed 
    player.sprite = 1
  end
  if btnp(3) then 
    player.dy = player.speed
    player.sprite = 1
  end
  
  if player.y<0 then
    player.y=0
  end
  if player.x<0 then
    player.x=0
  end
  if player.y>120 then
    player.y=120
  end
  if player.x>120 then
    player.x=120
  end
  
  check_collide()

  if not won() then
    check_move_bldr()

    player.y += player.dy
    player.dy = 0
    player.x += player.dx
    player.dx = 0
  end
end

function _draw()
  if not won() then
    cls()
    map(level_startx,0,0,0,16,16)
    spr(player.sprite, (player.x*8)-(level_startx*8), player.y*8)
  else
    t+=1
    if t%7==0 then
      cls()
      if t%2==0 then
        spr(24,60,60)
      else
        spr(25,60,60)
      end
    end
    if t%3==0 then
      print("you win!",50,40,t%16)
      print(":)",60,50,t%16)
    end
  end
end

function check_collide()
  if solid(player.x+player.dx,
           player.y+player.dy) then
    player.dx=0
    player.dy=0
  end
  if mget(player.x+player.dx,
          player.y+player.dy) == ladder_spr then
    next_level()
  end
end

function check_move_bldr()
  local posx = player.x+player.dx
  local posy = player.y+player.dy
  if is_bldr(posx, posy) then
    -- check if pushing to hole
    local new_bposx = posx+player.dx
    local new_bposy = posy+player.dy
    if player.dx > 0 then
      player.sprite = 18
    
    elseif player.dx < 0 then
      player.sprite = 19
    end
    if is_hole(new_bposx, new_bposy) then
      -- fill hole
      sfx(0)
      mset(new_bposx,
           new_bposy,
           filled_hole_spr)
      -- remove boulder
      mset(posx, posy, ground_spr)
    elseif is_bldr(new_bposx,new_bposy) then
      -- don't move boulder if
      -- there's a boudler
      -- in the way
      player.dx=0
      player.dy=0
    elseif not solid(new_bposx,new_bposy) then
      sfx(1)
      -- remove current boulder
      mset(posx, posy, ground_spr)
      -- mset new boulder pos
      mset(new_bposx,
           new_bposy,
           bldr_spr)
    else
      -- all other cases
      -- just don't move
      player.dx=0
      player.dy=0
    end
  end
end

function is_bldr(x,y)
  return mget(x,y) == bldr_spr
end

function is_hole(x,y)
  return mget(x,y) == hole_spr
end

function solid(x, y)

 -- grab the cell value
 val=mget(x, y)
 
 -- check if flag 0 is set (the
 -- orange toggle button in the 
 -- sprite editor)
 return fget(val, 0)
 
end

function ground(x, y)
 val=mget(x, y)
 return fget(val, 1)
end

function next_level()
  level+=1
  level_startx=level*16
  _init()
end

function won()
  if mget(level_startx,0) == 3 then
    return true
  end
  return false
end
