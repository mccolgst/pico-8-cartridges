pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
stage_y=40
max_missiles=2
max_babies=5
max_buildings=8
max_vehicles=3
time_remaining=120
done=false
t=0
score=0
fires={}
buildings={}
ppl={}
missiles={}
babies={}
vehicles={}
plx_scrls = {}
smoke={}
smoke.x=0
smoke.y=0
smoke.s=30
smoke.frames=4
baby_cd=30*5
missile_speed=1.5
fire_speed=2
title_screen=true
title_sfx=false
title_show_msg=1
title_y=0
heart_s=11
p = {}
p.speed=.75
p.dy=0
p.dx=0
p.s=1
p.frames=4
p.t=0
p.health=100
p.invuln=false
p.invuln_cd=0
p.fire_cd=0
p.x=128/2-12
p.y=128/2-14

// mode
// 0 = title screen
// 1 = game
// todo: tutorial screen?
//

mode=0

function _init()
  //music(0)
  if mode==0 then
    init_title()
  else
    init_game()
  end
end

function init_title()
  for i=1,max_babies do
    local b = create_baby()
    b.active=true
    b.x=rnd(128)
    b.y=rnd(128)
  end
end

function init_game()
  for i=0,max_buildings do
    create_buildings()
  end
  for i=0,max_missiles do
    create_missile()
  end
  new_plx_scrl(2,16,0,15,-.1,128)
  new_plx_scrl(2,16,0,15,-.2,176)
  new_plx_scrl(2,16,0,20,-.3,224)
end

function _update()
  t+=1
  if mode==0 then
    update_title()
  end
  if mode==1 then
    update_game()
  end
end

function update_title()
  p.t+=1
  if title_y<30 then
    title_y+=t/8
  end
  for b in all(babies) do
    update_baby(b)
  end
  if btnp(4) then
    mode+=1
    init_game()
    babies={}
  end
end
function update_game()
  if not done then
    if t%30==0 then 
      time_remaining-=1
    end
    p.dx=0
    p.dy=0
    if btnp(4) and p.t > p.fire_cd then
      shoot()
    end
    if btnp(5) then
      blink()
    end
    if btn(0) then
      p.dx=-p.speed
    elseif btn(1) then
      p.dx=p.speed
    end
  
    if btn(2) then
      // move player up
      p.dy=-p.speed
    elseif btn(3) then
      // move player down
      p.dy=p.speed
    else
      p.dy=0
    end
  
    if #babies < max_babies and baby_cd < t then
      create_baby()
    end
  
    if #vehicles < max_vehicles then
      create_vehicle()
    end
  
    if t%5==0 then
      p.s=1+p.t%p.frames
    end

    if p.x < 8 and not p.invuln then
      hurt_player()
    end
    if p.invuln_cd<p.t then
      p.invuln=false
    end
    for fire in all(fires) do
      fire.t+=1
      fire.s=33+fire.t%fire.frames
      fire.x+=fire.dx
      if fire.x>128 then
        fire.done=true
      end
    end
    for build in all(buildings) do
      update_building(build)
    end
    update_ppl()
    for m in all(missiles) do
      if check_collision(p,m) then
        hurt_player()
        m.x+=128+rnd(128)
        m.y=rnd(128)
        m.dx=1+rnd(missile_speed)
      end
      m.t+=1
      m.x-=m.dx
      m.s=38+m.t%m.frames
      if m.x < 0 then
        m.x+=128+rnd(128)
        m.y=rnd(128)
        m.dx=1+rnd(missile_speed)
      end
    end
    for fire in all(fires) do
      if fire.done then
        del(fires,fire)
      end
    end

    if t%100==0 then
      for i=0,max_buildings do
        create_buildings()
      end
    end

    for b in all(babies) do
      update_baby(b)
    end
  
    for v in all(vehicles) do
      update_vehicle(v)
    end
    for v in all(vehicles) do
      if v.x<0 then
        del(vehicles,v)
      end
    end
    foreach(plx_scrls, update_plx_scrl)
    p.y+=p.dy
    p.x+=p.dx
    p.t+=1
  end
end


function _draw()
  cls()
  if mode==0 then
    draw_title()
  else
    draw_game()
  end
end
function draw_title()
  spr(101,45,title_y,5,2)
    
  if title_y>30 then
    if title_sfx==false then
      title_sfx=true
      sfx(8)
    end
    if t%20==0 then
      title_show_msg*=-1
    end
    if t%7==0 then
      heart_s=11+t%2
    end
    spr(heart_s,128/2-6,128/2-20)
    if t%5==0 then
      p.s=1+p.t%p.frames
    end
    for b in all(babies) do
      spr(b.s,b.x,b.y)
    end
    spr(p.s,p.x,p.y)
    if title_show_msg>0 then
      print("press z to start",35,63,7)
    end
    print("z=fireball, x=blink",30,73,1)
    print("mother nature has summoned you",7,83,1)
    print("to destroy as much as you can",10,93,1)
    print("before time is up",30,103,1)
  end
end
function draw_game()
  for b in all(buildings) do
    spr(b.s,b.x,b.y)
  end
  for fire in all(fires) do
    spr(fire.s,fire.x,fire.y)
  end
  foreach(plx_scrls, draw_plx_scrl)
  
  for m in all(missiles) do
    spr(m.s,m.x,m.y)
  end
  for b in all(babies) do
    spr(b.s,b.x,b.y)
  end
  for v in all(vehicles) do
    spr(v.s,v.x,v.y)
  end
  if p.invuln and t%5==0 then
    p.s=2+p.t%p.frames
  end
  if p.t<p.fire_cd then
    draw_fire_cd()
  end
  if smoke.s<32 then
    spr(smoke.s,smoke.x,smoke.y)
    smoke.s+=1
  end
  spr(p.s,p.x,p.y)
  
  // draw fire chasing
  for i=5,16 do
    spr(flr(rnd(4))+22,0,i*8)
  end
  for pers in all(ppl) do
    pset(pers.x,pers.y,pers.c)
  end
  draw_hud()
  if p.health<=0 then
    game_over(true)
  elseif time_remaining<=0 then
    game_over(false)
  end
end

function blink()
  smoke.x=p.x
  smoke.y=p.y
  smoke.s=27
  if btn(2) then
    p.y-=15
  elseif btn(3) then
    p.y+=15
  end
  if btn(0) then
    p.x-=15
  elseif btn(1) then
    p.x+=15
  end
  for b in all(babies) do
    if b.active then
      b.x = p.x
      b.y = p.y
    end
  end
  sfx(9)
end

function shoot()
  local b = {}
  b.frame=0
  b.spr_l=33
  b.frames=4
  b.x=p.x+7
  b.y=p.y
  b.dx=fire_speed
  b.done=false
  b.t=0
  b.s=33
  p.fire_cd=(1*30)+p.t
  add(fires,b)
  
  for baby in all(babies) do
    if baby.active then
      local b = {}
      b.frame=0
      b.spr_l=33
      b.frames=4
      b.x=baby.x+7
      b.y=baby.y
      b.dx=fire_speed
      b.done=false
      b.t=0
      b.s=33
      add(fires,b)
    end  
  end
  sfx(1)
end

function create_buildings()
  local b = {}
  b.x=p.x+128+rnd(128)
  b.y=stage_y+rnd(128-stage_y)
  b.w=8
  b.h=8
  b.destroyed=false
  sp=flr(rnd(3))
  b.s=sp+49
  add(buildings,b)
end

function create_baby()
  local b = {}
  b.x=rnd(128)+128
  b.y=stage_y+rnd(128-stage_y)
  b.w=8
  b.h=8
  b.t=0
  b.dx=0
  b.dy=0
  b.s=54
  b.frames=2
  b.active=false
  add(babies,b)
  return b
end

function update_baby(b)
  b.t+=1
  if b.x<0 then
    b.x=128+rnd(128)
    b.y=stage_y+rnd(128-stage_y)
  end
  if t%15==0 then 
    if b.active then
      b.frames=4
      b.s=6+b.t%b.frames
    else
      b.frames=2
      b.s=54+b.t%b.frames
    end
  end
  
  if check_collision(p,b) then
    b.active=true
  end
  for m in all(missiles) do
    if check_collision(m,b) then
      del(babies,b)
      sfx(10)
    end
  end
  if b.active then
    if t%15==0 then
      b.speed=rnd(1)
      if p.x > b.x then
        b.dx=b.speed
      else
        b.dx=-b.speed
      end
      if p.y > b.y then
        b.dy=b.speed
      else
        b.dy=-b.speed
      end
    end
  else
    b.x-=1
  end
  
  b.x+=b.dx
  b.y+=b.dy
end

function create_vehicle()
  v = {}
  v.x=rnd(128)+128
  v.dx=(1+rnd(missile_speed))/2
  v.y=stage_y+rnd(128-stage_y)
  v.s=59+flr(rnd(3))
  v.w=8
  v.h=4
  v.dead=false
  add(vehicles,v)
end

function update_vehicle(v)
  if v.dead then
    v.dx=1
  end
  v.x-=v.dx
  if not v.dead then
    for b in all(babies) do
      if check_collision(b,v) then
        v.s-=16
        v.dead=true
        score+=flr(rnd(20))
        sfx(0)
      end
    end
    if check_collision(p,v) then
      v.s-=16
      v.dead=true
      score+=flr(rnd(20))
      sfx(0)
    end
  end
end

function create_missile()
  m = {}
  m.x=rnd(128)+128
  m.dx=1+rnd(missile_speed)
  m.y=rnd(128)
  m.frames=4
  m.s=38
  m.t=0
  m.w=8
  m.h=4
  add(missiles,m)
end

function check_collision(thing1, thing2)
  if thing1.x < thing2.x+thing2.w and
     thing1.x+8 > thing2.x and
     thing1.y+8 > thing2.y and
     thing1.y < thing2.y+thing2.h and
     thing1.y+8 > thing2.y then
     
    return true
  end
  return false
end

function draw_hud()

  //health
  rect(10,2,10+36,6,6)
  rectfill(11,3,11+p.health*.36-1,5,3)
  //score
  print("score",80,3,6)
  print(score,90,10,6)
  print("time",54,3)
  print(time_remaining,56,10,6)
end

function draw_fire_cd()
  rectfill(p.x-5,p.y+13, (p.x-5+(p.fire_cd-p.t)/2), p.y+12,3)
end

function hurt_player()
  if p.invuln==false then
    p.health-=33
    p.invuln=true
    p.invuln_cd=(3*30)+p.t
    sfx(2)
  end
end
function update_building(build)
  if build.destroyed==false and check_collision(p,build) then
    p.dx=-1
  end
  build.x-=1

  if build.destroyed == false then
    for fire in all(fires) do
    if check_collision(fire, build) then
      fire.done=true
      build.destroyed=true
      score+=flr(rnd(5))
      sfx(0)
      build.s=flr(rnd(4))+16
      //create running ppl
      create_ppl(build.x, build.y)  
    end
    end
  end
  
  if build.x<0 then
    del(buildings,build)
    return
  end
end

function create_ppl(x,y)
  for i=0,flr(rnd(15)) do
    local pers = {}
    pers.t=0
    pers.x=x
    pers.y=y
    pers.w=1
    pers.h=1
    pers.c=7
    pers.dx=rnd(.2)
    pers.dy=rnd(.2)
    if flr(rnd(2))>=1 then
      pers.dx*=-1
    end
    if flr(rnd(2))>=1 then
      pers.dy*=-1
    end
    
    add(ppl,pers)
  end
end

function update_ppl()
  for pers in all(ppl) do
    pers.t+=1
    pers.x-=1
    pers.x+=pers.dx
    pers.y+=pers.dy
    for baby in all(babies) do
      if check_collision(baby,pers) then
        pers.dx=0
        pers.dy=0
        pers.c=8
        score+=1
      end
    end
    if check_collision(p,pers) then
      pers.dx=0
      pers.dy=0
      pers.c=8
      score+=1
      if p.health<100 then
        p.health+=.2
      end
    end
  end
  
  for pers in all(ppl) do
    if pers.x < 0 then
      del(ppl, pers)
    end
  end
end

function game_over(lost)
  cls()
  done=true
  print("you have",128/2-30,128/2,6)
  if lost then
    print("been vanquished!",128/2-25,128/2+10,6)
  else
    print("punished humanity!",128/2-25,128/2+10,6) 
  end
  print("score: "..score,128/2-25, 128/2+20,6)
end

function new_plx_scrl(h, w, x, y, spd, sp)
  local plx_scrl = {}
  plx_scrl.w = w
  plx_scrl.h = h
  plx_scrl.x = x
  plx_scrl.y = y
  plx_scrl.sp = sp
  plx_scrl.spd = spd
  
  // second one is for smooth cycle
  local plx_scrl2 = {}
  plx_scrl2.w = w
  plx_scrl2.h = h
  plx_scrl2.x = x+(w*8)
  plx_scrl2.y = y
  plx_scrl2.sp = sp
  plx_scrl2.spd = spd
  
  add(plx_scrls, plx_scrl)
  add(plx_scrls, plx_scrl2)
end

function update_plx_scrl(plx_scrl)
  plx_scrl.x+=plx_scrl.spd
  if plx_scrl.x<=-(plx_scrl.w*8) then
    plx_scrl.x=plx_scrl.w*8-2
  end
end

function draw_plx_scrl(plx_scrl)
  spr(plx_scrl.sp,
      plx_scrl.x,
      plx_scrl.y,
      plx_scrl.w,
      plx_scrl.h)
end
